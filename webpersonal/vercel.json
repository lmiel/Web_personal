{
    "builds": [
      {
        "src": "webpersonal/wsgi.py",
        "use": "@vercel/python"
      },
      {
          "src": "build_files.sh",
          "use": "@vercel/static-build",
          "config": { "distDir": ".vercel/output" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "webpersonal/wsgi.py"
      }
    ]
}
